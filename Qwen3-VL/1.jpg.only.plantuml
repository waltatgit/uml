@startuml
' Assuming 'n' on associations means '*' (many) as per UML convention for unspecified multiplicity.
' No visibility specified for attributes; defaulting to public (no prefix).

class Order {
  date
  prepaid
  number
  price
}

class Customer {
  name
  address
  ratings
}

class OrderLine {
  product
  price
  quantity
}

class Corporate {
  name
  rating
  limit
}

class Personal {
  id
  contact
}

Order "1" -- "n" Customer
Order "1" -- "n" OrderLine
Customer <|-- Corporate
Customer <|-- Personal

@enduml