}

class Inventory {
  -type: str
  -status: str
}

class Rooms {
  -number: int
  -location: string
}

class Food {
  -id
  -name
}

class Chef {
  -name: str
  -id: int
  -location: str
}

class Receptionist {
  -name: string
  -id: int
  -number: string
  -location: string
}

class Bill {
  -number: int
