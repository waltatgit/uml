@startuml
' TODO: Assuming all classes are concrete; no abstract indicators visible.
' TODO: Assuming "coshtender" in Cash class is a typo for "cashTender".

class Customer {
  name
  address
}

class Order {
  date
  status
}

class Payment {
  amount
}

class Credit {
  number
  type
  expDate
}

class Cash {
  cashTender
}

class Check {
  name
  bank
}

Customer "1" -- "0..*" Order
Order "1" -- "1..*" Payment
Payment <|-- Credit
Payment <|-- Cash
Payment <|-- Check

@enduml