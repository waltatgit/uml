@startuml
' TODO: Assuming "String" and "double" are the intended types for attributes; no visibility specified, so using default (public).
' TODO: Assuming "manages" and "owner" are association roles; multiplicities are inferred from diagram.

class Cattery {
  name : String
  address : String
}

class Cat {
  breed : String
  age : double
  id : String
}

class Person {
  name : String
}

Cattery "0..1" *-- "*" Cat : residents
Cattery "1" *-- "*" Person : manages
Cat "1" -- "*" Person : owner

@enduml