@startuml
' Assumptions:
' - No visibility modifiers were indicated; attributes are shown without visibility.
' - Association appears undirected; kept undirected with multiplicities "1" and "*".
' - Label "items" is treated as the association role name on the Order side.
left to right direction

class Order {
  orderNumber: String
  total: double
}

class "Order Item" {
  item: String
  quantity: int
  price: double
}

Order "1" -- "*" "Order Item" : items
@enduml