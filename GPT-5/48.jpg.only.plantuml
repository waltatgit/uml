@startuml
' Assumptions:
' - Types normalized: "str" -> String.
' - The association between Account and Account Holder shows roles "holder" and "accounts" with multiplicities 1 (holder) to 1..* (accounts).
' - Class names include spaces as drawn.

left to right direction

class "Account Holder" {
  id: int
  address: String
}

class Account {
  balance: float
}

class "Individual Holder" {
  name: String
  SSN: String
}

class "Corporate Holder" {
  contact: String
}

"Individual Holder" --|> "Account Holder"
"Corporate Holder" --|> "Account Holder"

"Account Holder" "1" -- "1..*" Account : holder/accounts
@enduml