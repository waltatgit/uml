' TODO: Some multiplicities are hard to read; best-effort assumptions noted on each association.

left to right direction

class Firm {
  name: string
}

class Adminin {
  name: string
  address: string
}

class Shareholder {
  shares: int
  id: string
  name: string
}

class Assembly {
  startTime
  endTime
  percShares
  maxNoDeleg
}

class Delegation {
}

class Voting {
  name: str
  choiceA: str
  choiceB: str
}

class Vote {
  choice: str
}

' Relationships
Firm "1" -- "1" Adminin
Firm "1" -- "*" Shareholder

Adminin "0..*" -- "1" Assembly

Shareholder "0..*" -- "1" Assembly

Shareholder "0..*" -- "1" Delegation
Assembly  "0..*" -- "1" Delegation

Assembly "0..*" -- "1" Voting
Voting  "1" -- "0..1" Vote

Shareholder -- Vote
@enduml